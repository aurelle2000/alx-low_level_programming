#include <stdio.h>

int main(void)

{
	char a;
	int b;
	long int c;
	long long int d;
	float e;
	
	printf("Size of a char: %d byte(s)\n", sizeof(a));
	print("Size of an int: %d byte(s)\n", sizeof(b));
	print("Size of an long int: %d byte(s)\n", sizeof(c));
	print("Size of an long long int: %d byte(s)\n", sizeof(d));
	print("Size of an float: %d byte(s)\n", sizeof(e));

	return (0);

}
